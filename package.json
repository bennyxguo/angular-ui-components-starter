{
  "name": "@ui-kit/ui-components-project",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "npm run storybook",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod && npm run postbuild",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "postbuild": "npx tailwindcss -c projects/amp-digital-eno/aem-amp-ng-components/tailwind.config.js -i projects/amp-digital-eno/aem-amp-ng-components/style.css -o dist/amp-digital-eno/aem-amp-ng-components/style.min.css --minify && cp -f dist/amp-digital-eno/aem-amp-ng-components/style.min.css .storybook/style.min.css",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "angular-svg-icon": "^14.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.6",
    "@angular/cli": "~14.1.0",
    "@angular/compiler-cli": "^14.0.0",
    "@babel/core": "^7.18.9",
    "@compodoc/compodoc": "^1.1.19",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/angular": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@types/jasmine": "~4.0.0",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "css-selector-tokenizer": "^0.8.0",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^14.0.0",
    "postcss": "^8.4.14",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.1.6",
    "typescript": "~4.7.2"
  }
}
